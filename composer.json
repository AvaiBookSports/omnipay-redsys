{
    "name": "avaibooksports/omnipay-redsys",
    "type": "library",
    "description": "Redsys driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "redsys",
        "servired",
        "sermepa",
        "redirect",
        "webservice",
        "web service",
        "soap"
    ],
    "homepage": "https://github.com/PatronBase/omnipay-redsys",
    "license": "MIT",
    "authors": [
        {
            "name": "Leith Caldwell",
            "email": "leith@codedruids.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Redsys\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\Redsys\\" : "tests/" }
    },
    "require": {
        "php": "^7.4|8.*",
        "omnipay/common": "^3.4",
        "avaibooksports/redsys-messages": "^0.2"
    },
    "require-dev": {
        "omnipay/tests": "^4.1",
        "squizlabs/php_codesniffer": "^3.13",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^2.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "php-http/react-adapter": "^3.0"
    },
    "suggest": {
        "ext-openssl": "Required for hashing functions to check message signatures"
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "7.4"
        }
    }
}
